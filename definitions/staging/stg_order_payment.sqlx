config {
  type: "view",
  schema: "staging",
  description: "Cleaned version of customer table"
}

with source as (
    select * from ${ref("olist_order_payments_dataset")}
),

renamed as (
    select
        order_id,
        payment_sequential,
        payment_type,
        payment_installments,
        payment_value,

    from source
)

select * from renamed